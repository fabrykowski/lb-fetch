    "esbuild": "^0.25.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "files": [
    "/build",
    "/npm-shrinkwrap.json"
  ],
  "keywords": [
    "fetch",
    "request",
