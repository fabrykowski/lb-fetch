{
  "name": "lb-fetch",
  "version": "1.0.1",
  "description": "Load balanced fetch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabrykowski/lb-fetch.git"
  },
  "homepage": "https://github.com/fabrykowski/lb-fetch#readme",
  "bugs": {
    "url": "https://github.com/fabrykowski/lb-fetch/issues"
  },
  "type": "module",
